{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/routes/auth/.*",
      "dest": "index.js"
    },
    {
      "src": "/routes/inventory/.*",
      "dest": "index.js"
    },
    {
      "src": "/routes/profile/.*",
      "dest": "index.js"
    }
  ]
}